{
	"manifest_version": 3,
	"name": "YouTube Ad Skipper",
	"version": "1.0",
	"description": "YouTube広告が再生され始めた際にページバックしてまた戻ります",
	"permissions": [
	  "tabs",
	  "activeTab",
	  "scripting"
	],
	"background": {
	  "service_worker": "background.js"
	},
	"content_scripts": [
	  {
		"matches": ["*://www.youtube.com/*"],
		"js": ["content.js"]
	  }
	],
	"icons": {
	  "16": "16.png",
	  "48": "48.png",
	  "128": "128.png"
	}
  }